{
  "expo": {
    "name": "FlixFlex",
    "slug": "flix-flex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/flix-flex.png",
      "resizeMode": "contain",
      "backgroundColor": "#a855f7"
    },
    "assetBundlePatterns": ["**/*"],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-web-browser", ["expo-splash-screen"]],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "ios": {
      "config": {
        "usesNonExemptEncryption": false
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.nroussama.flixflex"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon/foreground.png",
        "backgroundImage": "./assets/adaptive-icon/background.png"
      },
      "package": "com.nroussama.flixflex"
    }
  }
}
